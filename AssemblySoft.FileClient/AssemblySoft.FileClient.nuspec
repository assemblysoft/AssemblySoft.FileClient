<?xml version="1.0"?>
<package>
  <metadata>
    <id>AssemblySoft.FileClient</id>
    <version>1.0.0.3</version>
    <title>AssemblySoft.FileClient</title>
    <authors>Carl Randall</authors>
    <owners>AssemblySoft</owners>
    <licenseUrl>https://opensource.org/licenses/MIT</licenseUrl>
    <projectUrl>https://www.assemblysoft.com/packages</projectUrl>
